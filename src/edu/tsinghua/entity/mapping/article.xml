<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="edu.tsinghua.entity.Article" table="article">
		<id name="artId" type="int" column="artId">
			<generator class="increment"></generator>
		</id>
		<property name="title" type="java.lang.String" ></property>
		<property name="artDate" type="java.lang.String"></property>
		<property name="artSummary" type="java.lang.String"></property>
		<property name="content" type="java.lang.String"></property>
		
	
	</class>

</hibernate-mapping>
    